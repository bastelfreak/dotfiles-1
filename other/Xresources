urxvt*foreground: grey
urxvt*background: black
urxvt*scrollBar: false
urxvt*font: xft:Monospace:pixelsize=11
!If you want transparent and shading uncomment the following lines
!urxvt*.transparent: true
!urxvt*.shading: 40



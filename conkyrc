out_to_console yes
out_to_x no
background no
update_interval 2
total_run_times 0
use_spacer none


TEXT

${execi 300 /usr/bin/weather "EUR|DE|GMXX6207|Clausthal"} :: Network ${if_up eth0} Wired Up ${upspeed eth0} Down ${downspeed eth0} $endif${if_up wlan0} Wifi Up ${upspeed wlan0} Down ${downspeed wlan0}$endif :: Battery Status $battery  :: Ram Usage $mem/$memmax - $memperc% :: CPU Usage $cpu%  :: Uptime $uptime :: ${time %Y-%m-%d %H:%M}  




